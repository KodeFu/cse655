# Read Memory and Register Data Hazard
regi r0, 10   # r0 = 10
regi r1, 25   # r1 = 25
memr r1, r0   # r3 = *r0
mulr r0, r3   # r0 = r3 * r0
              # ** hazard, since previous instruction reads memory in the MEM, then uses WB to write value to r3
rand r4       # r4 = rand()
rand r5       # r4 = rand()